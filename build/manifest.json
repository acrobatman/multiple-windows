{
  "background": {
    "scripts": [ "Index.js" ]
  },
  "manifest_version": 2,
  "name": "Work to Multiple Windows",
  "description": "This project is a chrome extension. It provide to create windows to different sizes and multiple for front-end developers.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon-16.png"
  },
  "icons": {
    "256": "icon-256.png",
    "128": "icon-128.png",
    "48": "icon-48.png",
    "16": "icon-16.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "*://*/*/*",
    "<all_urls>"
  ],
  "web_accessible_resources": ["Index.js", "bundle.js", "*.ttf", "*.woff", "*.woff2", "*.png", "*.eot", "*.svg", "http://*", "https://*"]
}
